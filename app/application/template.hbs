{{!-- ember-cli-materialize modal container --}}
{{md-modal-container}}
{{#md-navbar name='MES Time Sheets' class='brand-logo' homeRoute='index'}}
  {{#link-to 'protected' tagName='li'}}
    {{link-to 'Protected' 'protected'}}
  {{/link-to}}
{{#if session.isAuthenticated}}
  {{md-btn text=session.content.secure.email action='invalidateSession' class="btn"}}
{{else}}
  {{#link-to 'login' tagName='li'}}
    {{link-to 'Login' 'login' class="btn"}}
  {{/link-to}}
{{/if}}
{{/md-navbar}}
{{outlet}}
