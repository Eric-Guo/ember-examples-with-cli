{{!-- ember-cli-materialize modal container --}}
{{md-modal-container}}
{{#md-navbar name='Time Sheets' class='brand-logo' homeRoute='index'}}
  {{#link-to 'new' tagName='li'}}
    {{link-to 'New' 'new' class="btn-large lime"}}
  {{/link-to}}
  &nbsp;&nbsp;
{{#if session.isAuthenticated}}
  {{md-btn text=session.content.secure.email action='invalidateSession' class="btn"}}
{{else}}
  {{#link-to 'login' tagName='li'}}
    {{link-to 'Login' 'login' class="btn"}}
  {{/link-to}}
{{/if}}
{{/md-navbar}}
{{outlet}}
